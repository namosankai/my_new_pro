<body>
    <div class="main-excel-order">
        <div class="container">
            
                <div class="section-title">
                    <h1>商品詳細</h1>
                
                        <div class="order-index-item">
                            <div class="order-list"></div>
                                <% @items.each do |item| %>
                                <li>
                                    <p><%= item.name %></p>
                                    <p><%= item.price %>円</p>
                                    <p><%= item.cost %>円</p>
                                    <p><%= item.taxin %>円</p>
                            
                                </li>
                                
                                
                            </div>
                        <% end %>
                        </div>
                
                </div>
        </div>

    </div>
</body>